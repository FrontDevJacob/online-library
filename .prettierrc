{
   "tabWidth": 3,
   "printWidth": 100,
   "semi": false,
   "singleQuote": true,
   "arrowParens": "avoid",
   "trailingComma": "es5",
   "plugins": ["./node_modules/@trivago/prettier-plugin-sort-imports"],
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "importOrder": [
      "<THIRD_PARTY_MODULES>",

      "@online-library/config",
      "@online-library/core",
      "@online-library/logic",

      "^dotenv(.*)",
      "^./config/aliases$",
      "^config$",

      "^swagger$",

      "^database(.*)",

      "^shared$",

      "^gql(.*)",

      "^middlewares(.*)",

      "^routes(.*)",

      "^@redux$",
      "^@redux/reducers(.*)",

      "^styles(.*)",

      "^assets(.*)",

      "(.*)/styled$",

      "^components/shared$",
      "^components(.*)",
      "^./components$",

      "^./modules$",

      "^hooks$",
      "(.*)/hooks$",

      "^helpers$",
      "(.*)/helpers$",

      "^utils$",
      "(.*)/utils$",

      "^types(.*)",
      "(.*)/types$",

      "^[./]"
   ]
}
