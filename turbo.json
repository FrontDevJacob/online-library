{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": { "cache": false },
      "lint": { "outputs": [] },
      "test": { "outputs": [] },
      "docs": {
         "dependsOn": ["^lib:build"],
         "outputs": ["swagger.json"]
      },
      "build": {
         "dependsOn": ["^lib:build", "docs", "test"],
         "outputs": ["dist/**", "build/**"]
      },
      "lib:build": {
         "dependsOn": ["^lib:build", "dts"],
         "outputs": ["dist/**"]
      },
      "dts": { "outputs": ["dist/**"] }
   }
}
