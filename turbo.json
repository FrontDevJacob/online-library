{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": { "cache": false },
      "lint": { "outputs": [] },
      "test": { "outputs": [] },
      "e2e": { "outputs": [] },
      "docs": {
         "dependsOn": ["^lib:build"],
         "outputs": ["swagger.json"]
      },
      "build": {
         "dependsOn": ["^lib:build", "docs", "test", "e2e"],
         "outputs": ["dist/**"]
      },
      "lib:build": {
         "dependsOn": ["^lib:build"],
         "outputs": ["dist/**"]
      }
   }
}
