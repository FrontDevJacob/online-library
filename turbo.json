{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": { "cache": false },
      "lint": { "outputs": [] },
      "test": { "outputs": [] },
      "docs": { "outputs": ["swagger.json"] },
      "build": {
         "dependsOn": ["^build", "docs", "lint", "test"],
         "outputs": ["build", "dist"]
      }
   }
}
