{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": { "cache": false },
      "lint": { "outputs": [] },
      "test": { "outputs": [] },
      "docs": { "dependsOn": ["^lib:build"], "outputs": ["swagger.json"] },
      "build": {
         "dependsOn": ["^lib:build", "docs", "test"],
         "outputs": ["build", "dist"]
      },
      "lib:build": {
         "dependsOn": ["^lib:build"],
         "outputs": ["dist"]
      }
   }
}
