{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": {
         "outputs": []
      },
      "check": {
         "outputs": []
      },
      "test": {
         "dependsOn": ["check"],
         "outputs": []
      },
      "build": {
         "dependsOn": ["^build", "check", "test"],
         "outputs": ["build", "dist"]
      },
      "deploy": {
         "dependsOn": ["build", "check", "test"],
         "outputs": []
      }
   }
}
