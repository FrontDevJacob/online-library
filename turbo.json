{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "dev": { "cache": false },
      "lib:dev": { "cache": false },
      "lint": { "outputs": [] },
      "test": { "dependsOn": ["lib:build"], "outputs": [] },
      "docs": { "dependsOn": ["lib:build"], "outputs": ["swagger.json"] },
      "build": {
         "dependsOn": ["^lib:build", "test", "lint", "docs"],
         "outputs": ["build", "dist"]
      },
      "lib:build": { "outputs": ["dist"], "dependsOn": ["^lib:build"] }
   }
}
